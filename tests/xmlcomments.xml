    <id>2</id>
    <revision>
	    <text xml:space="preserve">
	    &lt;!--{{Citations missing}} begin--&gt;{{ambox
	    | type  = content
	    | text  = '''This {{{1|article or section}}} is missing [[Wikipedia:Citing sources|citations]] or needs [[Wikipedia:Footnotes|footnotes]].'''&lt;br/&gt;&lt;small&gt;Using [[Wikipedia:inline citations|inline citations]] helps guard against copyright violations and factual inaccuracies. {{#if:{{{date|}}}| ''({{{date}}})''|}}&lt;/small&gt;
	    }}&lt;includeonly&gt;{{#if:{{{date|}}}|{{{category|[[Category:Articles with unsourced statements since {{{date}}}]]}}}|{{{category|[[Category:Articles with unsourced statements]]}}}}}{{{category|[[Category:All articles with unsourced statements]]}}}&lt;/includeonly&gt;&lt;!--{{Citations missing}} end--&gt;&lt;noinclude&gt;{{documentation}}&lt;/noinclude&gt;
	    </text>
    </revision>
